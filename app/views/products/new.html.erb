<h1>Products#new</h1>
<p>Find me in app/views/products/new.html.erb</p>
<%= form_with scope: :product, url: products_path, local: true do |form| %>
  <p>
    <%= form.label :title %><br>
    <%= form.text_field :title %>
  </p>
 
  <p>
    <%= form.label :text %><br>
    <%= form.text_area :text %>
  </p>
 
  <p>
    <%= form.submit %>
  </p>
<% end %>